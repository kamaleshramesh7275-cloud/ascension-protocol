{
    "buildCommand": "npm run vercel-build",
    "outputDirectory": "dist",
    "functions": {
        "api/**/*.js": {
            "maxDuration": 10
        },
        "api/**/*.ts": {
            "maxDuration": 10
        }
    },
    "rewrites": [
        {
            "source": "/((?!api/.*).*)",
            "destination": "/index.html"
        }
    ]
}