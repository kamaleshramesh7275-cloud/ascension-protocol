{
    "buildCommand": "npm run vercel-build",
    "outputDirectory": "dist",
    "framework": null,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index"
        },
        {
            "source": "/((?!api/.*|assets/.*|favicon.ico|.*\\..*).*)",
            "destination": "/index.html"
        }
    ],
    "functions": {
        "api/**/*.ts": {
            "maxDuration": 10
        }
    }
}