{
    "buildCommand": "npm run vercel-build",
    "outputDirectory": "dist",
    "framework": null,
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index"
        }
    ],
    "rewrites": [
        {
            "source": "/((?!api/.*|assets/.*|favicon.ico|.*\\..*).*)",
            "destination": "/index.html"
        }
    ],
    "functions": {
        "api/**/*.js": {
            "maxDuration": 10
        },
        "api/**/*.ts": {
            "maxDuration": 10
        }
    }
}