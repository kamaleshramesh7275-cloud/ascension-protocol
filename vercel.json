{
    "buildCommand": "npm run vercel-build",
    "outputDirectory": "dist",
    "framework": null,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/((?!api/.*|assets/.*|favicon.ico|.*\\..*).*)",
            "destination": "/index.html"
        }
    ]
}